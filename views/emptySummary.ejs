<% include partials/sidebarHeader %>
<h2>No Plan Summary</h2>

<% var date = new Date(); %>
<% var curTime = Date.parse(date); %>
<% if(team.endTime - curTime > totalTime - sprintEnd[sprintNo-1]){ %>
    <form class="" action="/<%=team.id%>/<%=sprintNo%>/planSummary" method="get">
      <h4 align="center" class="h4text">Please Create a Plan Summary</h4>
      <button type="submit" id="SubBut" name="button">Create</button>
    </form>
<% } %>

<% include partials/sidebarFooter %>
<% include partials/footer %>
