<!DOCTYPE html>
<html lang="en" dir="ltr">
  <head>
    <meta charset="utf-8">
    <title>Teams</title>
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/css/bootstrap.min.css">
    <link rel="stylesheet" href="/stylesheets/my_list.css">
  </head>
  <body>
    <% include partials/navbar %>
    <div class="my-list overflow-auto">
      <% if (teams.length != 0){ %>
        <h2>Teams for Session <%= session %></h2>
        <ul>
          <% for(var i = 0; i < teams.length; i++){ %>
            <% url = "/" + teams[i].id + "/report" %>
            <li><a href="<%= url %>"><%= teams[i].name %></a></li>
          <% } %>
        </ul>
      <% } else {%>
        <h2>No teams in Session <%= session %></h2>
      <% } %>
    </div>
  </body>
</html>
